select t.num_bem existe
     from GENERICO.BEM_PATRIMONIAL_INVENTARIO_CE t
WHERE t.num_bem = :num_bem;



----------------------

SELECT 'QTD. DISPONIVEL' EQUIPAMENTOS,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 1, OTE.NOM_ABREVIADO_PESQUISA)) BOMBA,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 2, OTE.NOM_ABREVIADO_PESQUISA)) BIPAP,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 3, OTE.NOM_ABREVIADO_PESQUISA)) ULTRASSOM,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 4, OTE.NOM_ABREVIADO_PESQUISA)) MONITOR,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 5, OTE.NOM_ABREVIADO_PESQUISA)) VENTILADOR,
       COUNT(DECODE(T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO, 6, OTE.NOM_ABREVIADO_PESQUISA)) VENOSCOPIO    
 FROM GENERICO.BEM_PATRIMONIAL_INVENTARIO_CE t
 JOIN GENERICO.ORDEM_SERVICO_TIPO_EMPRESTIMO OTE
 ON OTE.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO = T.SEQ_ORDEM_SERV_TIPO_EMPRESTIMO
 GROUP BY T.NOM_ABREVIADO_PESQUISA;

SELECT * FROM GENERICO.ORDEM_SERVICO_TIPO_EMPRESTIMO OTE;
 
 SELECT * FROM V$VERSION;
